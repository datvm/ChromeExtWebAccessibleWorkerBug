{
    "manifest_version": 3,
    "name": "Test Chrome Worker",
    "version": "1.0",
    "background": {
        "service_worker": "/background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": { },
    "web_accessible_resources": [
        {
            "resources": [
                "/worker.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}